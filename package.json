    "@nuxt/content": "^2.12.1",
    "@nuxt/fonts": "^0.5.1",
    "@nuxt/image": "^1.6.0",
    "@nuxt/ui-pro": "^1.1.0",
    "dayjs": "^1.11.10",
    "nuxt": "^3.11.2",
    "nuxt-og-image": "^3.0.0-rc"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "^0.2.0",
    "@nuxthq/studio": "^1.0.13",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "@vue/eslint-config-prettier": "^9.0.0",
